  pod 'SwiftyJSON'
  pod 'Kanna'

  pod 'RxSwift'
  pod 'RxCocoa'

  pod 'IQKeyboardManagerSwift'
  pod 'SwipeCellKit'
  pod 'MarqueeLabel'
  pod 'DifferenceKit'

  pod 'Hue'

  pod 'DeviceKit'

  pod 'SwiftFormat/CLI'
  pod 'SwiftLint'

  target 'iOmicTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'RxBlocking'
    pod 'RxTest'
  end

  target 'iOmicUITests' do
    # Pods for testing
  end

